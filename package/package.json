{
  "name": "do-functions",
  "version": "1.0.1",
  "author": "Brian Evans",
  "type": "module",
  "repository": "https://github.com/mrbrianevans/do-functions",
  "description": "A helper to develop and deploy serverless functions to Digital Ocean.",
  "keywords": [
    "digital-ocean-functions",
    "serverless-functions",
    "digital-ocean"
  ],
  "packageManager": "pnpm@7.11.0",
  "homepage": "https://mrbrianevans.github.io/do-functions",
  "license": "MIT",
  "exports": {
    ".": {
      "default": "./wrapper/wrapper.js",
      "types": "./wrapper/wrapper.d.ts"
    },
    "./schema": {
      "default": "./wrapper/wrapperWithSchema.js",
      "types": "./wrapper/wrapperWithSchema.d.ts"
    }
  },
  "bin": {
    "do-functions": "./cli/build.js"
  },
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --build --watch",
    "clean": "tsc --build --clean",
    "test": "node --test",
    "prepack": "tsc --build"
  },
  "dependencies": {
    "@fastify/error": "^3.0.0",
    "esbuild": "^0.15.9",
    "jsonschema": "^1.4.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.7.4"
  }
}
